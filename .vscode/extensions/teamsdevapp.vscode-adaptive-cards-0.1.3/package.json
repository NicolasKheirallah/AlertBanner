{
	"name": "vscode-adaptive-cards",
	"displayName": "Adaptive Card Previewer",
	"description": "A VS Code extension to preview adaptive cards for Microsoft 365 app",
	"publisher": "TeamsDevApp",
	"author": "Microsoft Corporation",
	"preview": true,
	"private": true,
	"version": "0.1.3",
	"icon": "assets/images/icon.png",
	"engines": {
		"vscode": "^1.80.0"
	},
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"Adaptive Card",
		"Microsoft 365"
	],
	"categories": [
		"Other",
		"Extension Packs",
		"Snippets",
		"Visualization"
	],
	"activationEvents": [
		"onLanguage:json"
	],
	"main": "./dist/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/OfficeDev/acpreviewer"
	},
	"l10n": "./l10n",
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"contributes": {
		"commands": [
			{
				"command": "vscode-adaptive-cards.preview",
				"title": "%commands.preview.title%",
				"category": "%commands.category%",
				"icon": {
					"light": "./assets/media/preview-light.svg",
					"dark": "./assets/media/preview-dark.svg"
				}
			},
			{
				"command": "vscode-adaptive-cards.previewToSide",
				"title": "%commands.previewToSide.title%",
				"category": "%commands.category%"
			},
			{
				"command": "vscode-adaptive-cards.newDataFile",
				"title": "%commands.newDataFile.title%",
				"category": "%commands.category%"
			}
		],
		"keybindings": [
			{
				"command": "vscode-adaptive-cards.preview",
				"key": "shift+ctrl+v",
				"mac": "shift+cmd+v",
				"when": "editorLangId == json"
			},
			{
				"command": "vscode-adaptive-cards.previewToSide",
				"key": "ctrl+k v",
				"mac": "cmd+k v",
				"when": "editorLangId == json"
			}
		],
		"configuration": [
			{
				"title": "Adaptive Card Previewer",
				"properties": {
					"adaptiveCardPreviewer.defaultTheme": {
						"type": "string",
						"enum": [
							"followVscodeTheme",
							"teamsLight",
							"teamsDark",
							"teamsHighContrast"
						],
						"enumItemLabels": [
							"%configuration.defaultTheme.followVscodeTheme%",
							"%configuration.defaultTheme.teamsLight%",
							"%configuration.defaultTheme.teamsDark%",
							"%configuration.defaultTheme.teamsHighContrast%"
						],
						"enumDescriptions": [
							"%configuration.defaultTheme.followVscodeTheme.description%",
							"%configuration.defaultTheme.teamsLight.description%",
							"%configuration.defaultTheme.teamsDark.description%",
							"%configuration.defaultTheme.teamsHighContrast.description%"
						],
						"default": "followVscodeTheme",
						"description": "%configuration.defaultTheme.description%"
					}
				}
			}
		],
		"languages": [
			{
				"id": "adaptive-card-previewer-output"
			}
		],
		"grammars": [
			{
				"language": "adaptive-card-previewer-output",
				"scopeName": "adaptive-card-previewer.output",
				"path": "./syntaxes/adaptive-card-previewer-output.tmLanguage"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run clean && npm-run-all --parallel prod:build:*",
		"compile": "tsc -p ./",
		"build": "npm run clean && npm-run-all --parallel dev:build:*",
		"watch": "npm-run-all --parallel dev:watch:*",
		"dev:build:extension": "webpack --mode development --devtool source-map --config webpack.extension.config.js",
		"dev:build:webview": "webpack --mode development --devtool source-map --config webpack.webview.config.js",
		"dev:watch:extension": "webpack --watch --mode development --devtool source-map --config webpack.extension.config.js",
		"dev:watch:webview": "webpack --watch --mode development --devtool source-map --config webpack.webview.config.js",
		"prod:build:extension": "webpack --mode production --devtool hidden-source-map --config webpack.extension.config.js",
		"prod:build:webview": "webpack --mode production --devtool hidden-source-map --config webpack.webview.config.js",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"clean": "rimraf dist",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": " eslint --max-warnings=0 . --ext ts --ext tsx",
		"format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.2",
		"@types/sanitize-html": "^2.9.0",
		"@types/uuid": "^9.0.2",
		"@types/vscode": "^1.80.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@vscode/test-electron": "^2.3.2",
		"css-loader": "^6.8.1",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-react": "^7.32.2",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"npm-run-all": "^4.1.5",
		"prettier": "2.8.8",
		"rimraf": "^5.0.1",
		"style-loader": "^3.3.3",
		"ts-loader": "^9.4.3",
		"typescript": "^5.1.6",
		"webpack": "^5.85.0",
		"webpack-cli": "^5.1.1"
	},
	"dependencies": {
		"@1js/ac-react-sdk": "^3.6.6",
		"@fluentui/react-combobox": "^9.5.1",
		"@fluentui/react-components": "^9.27.4",
		"@fluentui/react-northstar": "^0.66.5",
		"@vscode/extension-telemetry": "^0.8.3",
		"@vscode/l10n": "^0.0.16",
		"adaptivecards-templating": "^2.3.1",
		"fs-extra": "^11.1.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-markdown": "^8.0.7",
		"sanitize-html": "^2.11.0",
		"tslib": "^2.6.0",
		"uuid": "^9.0.0"
	},
	"__metadata": {
		"id": "501f669e-2876-4940-b58b-828889847d73",
		"publisherId": "e3c289db-0715-45e7-94ad-71c2cd459a8a",
